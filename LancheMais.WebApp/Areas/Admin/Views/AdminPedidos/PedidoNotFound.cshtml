@model int
@{
    ViewData["Title"] = "404 Not Found";
}
<div class="alert alert-danger mt-1 mb1">
    <h1>Erro 404 - Not Found</h1>
    <hr />
    <h2>Pedido @Model não foi encontrado!</h2>
</div>
<a asp-action="Index" asp-controller="AdminPedidos"
class=btn btn-outline-success>
<h2>Retornar para a listagem de Pedidos.</h2>
</a>